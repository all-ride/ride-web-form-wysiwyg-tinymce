{
    "tinymce": {
        "theme": "advanced",
        "relative_urls": true,
        "plugins": "advlink,paste,style,safari",
        "theme_advanced_buttons1": "bold,italic,formatselect,separator,bullist,numlist,outdent,indent,separator,link,unlink,anchor,separator,pastetext,pasteword,separator,code",
        "theme_advanced_buttons2": "",
        "theme_advanced_buttons3": "",
        "theme_advanced_blockformats": "p,h1,h2,h3,h4,h5",
        "theme_advanced_toolbar_location": "top",
        "theme_advanced_toolbar_align": "left",
        "theme_advanced_path_location": "bottom",
        "theme_advanced_resizing": true,
        "paste_create_paragraph": true,
        "paste_create_linebreaks": true,
        "paste_use_dialog": false,
        "paste_auto_cleanup_on_paste": true,
        "paste_convert_middot_lists": true,
        "paste_unindented_list_class": "",
        "paste_convert_headers_to_strong": false,
        "apply_source_formatting": true,
        "auto_reset_designmode": true,
        "extended_valid_elements": "p[class|id|style],a[href|name|target|title|onclick|class|id|style|rel],img[src|class|id|border=0|alt|title|hspace|vspace|width|height|align|onmouseover|onmouseout|name|style],hr[class|id|width|size|noshade|style],span[class|id|style],em[class|id|style],ul[class|id|style],li[class|id|style],iframe[src|width|height|frameborder]"
    }
}
